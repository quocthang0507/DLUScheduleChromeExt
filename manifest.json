{
  "name": "Thời khóa biểu DLU",
  "description": "Tiện ích giúp theo dõi thời khóa biểu Trường Đại học Đà Lạt",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_title": "Thời khóa biểu DLU",
    "default_icon": {
      "16": "/logo/schedule_16.png",
      "32": "/logo/schedule_32.png",
      "48": "/logo/schedule_48.png",
      "128": "/logo/schedule_128.png"
    },
    "default_popup": "pages/popup.html"
  },
  "icons": {
    "16": "/logo/schedule_16.png",
    "32": "/logo/schedule_32.png",
    "48": "/logo/schedule_48.png",
    "128": "/logo/schedule_128.png"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  },
  "host_permissions": [
    "http://qlgd.dlu.edu.vn/*"
  ],
  "options_page": "pages/options.html",
  "permissions": [
    "scripting",
    "storage",
    "unlimitedStorage"
  ]
}